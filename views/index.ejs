        <button class="send_message js_send_message not_valid"><span class="icon wood_brand">&#10133;</span></button>
        <button class="delete_all js_delete_all not_valid"><span class="icon">&#59177;</span></button>
      </div>
      <ul class="message_list js_message_list"></ul>
    </div>
    <div class="memo_wrap2">
